{
  "extends": [
    "airbnb",
    "airbnb-typescript",
    "next",
    "prettier",
    "plugin:@next/next/recommended"
  ],
  "parserOptions": {
    "project": ["./tsconfig.json"]
  },
  "plugins": [
    "baseui"
  ],
  "rules": {
    "baseui/deprecated-theme-api": "warn",
    "baseui/deprecated-component-api": "warn",
    "baseui/no-deep-imports": "warn",
    "react/jsx-props-no-spreading": 0
  }
}